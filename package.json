{
  "name": "postgis-geologic-map",
  "version": "0.1.0",
  "description": "A PostGIS helper library to manage a geologic map",
  "author": "Daven Quinn",
  "license": "MIT",
  "bin": "bin/geologic-map",
  "scripts": {
    "compile": "./bin/compile",
    "preinstall": "git submodule update --init; npm install -D linklocal; linklocal",
    "start": "./bin/geologic-map-serve-dev"
  },
  "browserslist": [
    "> 1%"
  ],
  "extensions": {
    "geologic-map-display": "file:./extensions/display",
    "fill-patterns": "file:./extensions/fill-patterns",
    "live-tiles": "file:./extensions/live-tiles",
    "demo-units": "file:./extensions/demo-units"
  },
  "dependencies": {
    "@mapbox/postgis-vt-util": "^1.2.1",
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "colors": "^1.1.2",
    "express": "^4.16.4",
    "glob": "^7.1.2",
    "glob-promise": "^3.3.0",
    "inquirer": "^4.0.1",
    "map-digitizer-server": "file:./extensions/server/map-digitizer-server",
    "mapbox-gl": "^0.53.1",
    "mapbox-gl-utils": "^0.26.1",
    "pg": "^7.11.0",
    "pg-promise": "^7.5.4",
    "pkg": "^4.4.0",
    "progress": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "sql-strip-comments": "0.0.23",
    "tsparser": "^1.0.2",
    "underscore": "^1.9.1",
    "yargs": "^10.0.3"
  },
  "pkg": {
    "assets": [
      "**/*.coffee",
      "extensions/*/package.json",
      "extensions/**/*.pug",
      "**/*.sql",
      "extensions/**/web/dist/*.*",
      "extensions/live-tiles/src/map-style/base-style.json",
      "node_modules/@mapbox/mbtiles/lib/**/*.*"
    ]
  },
  "devDependencies": {
    "coffeescript": "^2.4.1",
    "cssnano": "^4.1.10",
    "linklocal": "^2.8.2",
    "parcel-bundler": "^1.12.3",
    "stylus": "^0.54.5"
  }
}
