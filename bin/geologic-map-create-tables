#!/usr/bin/env coffee
glob = require 'glob-promise'
{resolve} = require 'path'
{proc} = require '../src/util'
__base = resolve __dirname, '..'
colors = require 'colors'

do ->
  try
    for fn in await glob('topology/fixtures/*.sql', cwd: __base)
      await proc(fn)
  catch err
    console.log "Exiting on #{err}".red
    process.exit()
      #await proc('extensions/map-digitizer.sql')
      #await proc('extensions/unit-hierarchy')
  process.exit()

