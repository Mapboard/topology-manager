#!/usr/bin/env coffee
glob = require 'glob-promise'
{resolve} = require 'path'
{db,sql} = require '../src/util'
__base = resolve __dirname, '..'

do ->
  try
    for fn in await glob('topology/fixtures/*.sql', cwd: __base)
      await sql(fn)
    await sql('extensions/map-digitizer')
  catch
    process.exit()
  process.exit()

